{
    "manifest_version": 3, 
    "name": "VA Rating Extension", 
    "description": "", 
    "version": "1.0", 
    "action": {
        "default_popup": "popup/popup.html"
    }, 
    "icons": {
        "16": "images/DD_Logo_16.png", 
        "32": "images/DD_Logo_32.png", 
        "48": "images/DD_Logo_48.png", 
        "128": "images/DD_Logo_128.png"
    }, 
    "background": {
        "service_worker": "background.js", 
        "type": "module"
    }, 
    "permissions": [
        "storage"
    ], 
    "host_permissions": [
        "https://api.va.gov/*"
    ]
}